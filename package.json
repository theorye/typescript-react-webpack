  "description": "Typescript React Webpack with Babel 7",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "clean": "rimraf dist",
    "build": "npm run clean && NODE_ENV=production webpack --config=config/webpack.prod.js",
